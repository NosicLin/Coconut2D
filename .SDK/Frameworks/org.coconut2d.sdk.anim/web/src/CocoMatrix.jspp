	{
		// counter-clockwise rotation around Z-axis (perpendicular to the screen)
		var c:Float = Math.cos(rz);
		var s:Float = Math.sin(rz);
		var t0:Float, t1:Float;

		t0 = __m.rc11;
		t1 = __m.rc21;
		__m.rc11 =  t0 * c + t1 * s;
		__m.rc21 = -t0 * s + t1 * c;

		t0 = __m.rc12;
		t1 = __m.rc22;
		__m.rc12 =  t0 * c + t1 * s;
		__m.rc22 = -t0 * s + t1 * c;

		t0 = __m.rc13;
		t1 = __m.rc23;
		__m.rc13 =  t0 * c + t1 * s;
		__m.rc23 = -t0 * s + t1 * c;

		t0 = __m.rc14;
		t1 = __m.rc24;
		__m.rc14 =  t0 * c + t1 * s;
		__m.rc24 = -t0 * s + t1 * c;

		__dirty = true;
	}

