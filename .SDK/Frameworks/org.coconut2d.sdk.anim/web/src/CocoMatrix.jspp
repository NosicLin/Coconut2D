	////////////////////////////////////////////////////////////////////////////////////////////////////
	public function refresh()
	{
		__data[0]  = __m.rc11;
		__data[1]  = __m.rc12;
		__data[2]  = __m.rc13;
		__data[3]  = __m.rc14;
		__data[4]  = __m.rc21;
		__data[5]  = __m.rc22;
		__data[6]  = __m.rc23;
		__data[7]  = __m.rc24;
		__data[8]  = __m.rc31;
		__data[9]  = __m.rc32;
		__data[10] = __m.rc33;
		__data[11] = __m.rc34;
		__data[12] = __m.rc41;
		__data[13] = __m.rc42;
		__data[14] = __m.rc43;
		__data[15] = __m.rc44;
		__dirty = false;
	}

