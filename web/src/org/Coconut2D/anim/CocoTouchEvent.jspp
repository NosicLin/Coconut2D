class CocoTouchEvent
{ 
	public var x :Number;
	public var y :Number;
	public var type :Number;
	public var time :Number;
	public var handled :Boolean;
	
	public function setEvent(e:HTMLEvent)
	{ 
		x = e.clientX;
		y = e.clientY; 
		type = 0; 
		handled = false;
		var d:Date = new Date();
		time = d.getTime();				
	}	
}
