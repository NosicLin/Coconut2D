#include "Animation1.jspp"

class StateAnim :CocoEngineState
{ 
	private var scene:Animation1;
	
	////////////////////////////////////////////////////////////////////////////////////////////////////
	public function Constructor()
	{
		scene = new Animation1();
	}
	
	////////////////////////////////////////////////////////////////////////////////////////////////////
	public function tick(engine:CocoEngine, gl:WebGLRenderingContext, time:Float)
	{
		super.tick(engine, gl, time); 		
		scene && scene.tick(gl, time);
	}
}

